{
  "name": "wss",
  "version": "1.0.0",
  "description": "Web Socket server that hosts connect to and Apache Pulsar to route messages to and from workflows",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:tsup",
    "build:tsup": "tsup",
    "dev": "tsx --require ./node_modules/dotenv/config src/index.ts",
    "start": "node -r @traceloop/instrument-opentelemetry dist/index.js"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@traceloop/instrument-opentelemetry": "^0.4.1",
    "evt": "^2.4.13",
    "uuid": "^9.0.0",
    "ws": "^8.11.0",
    "zod": "^3.20.2",
    "pulsar-client": "1.7.0"
  },
  "devDependencies": {
    "@trigger.dev/tsconfig": "workspace:*",
    "internal-bridge": "workspace:*",
    "internal-platform": "workspace:*",
    "internal-pulsar": "workspace:*",
    "@trigger.dev/common-schemas": "workspace:*",
    "@types/debug": "^4.1.7",
    "@types/node": "16",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.3",
    "dotenv": "^16.0.3",
    "rimraf": "^3.0.2",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
